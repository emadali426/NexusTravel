                                                                </h4>
                                                                @*<a href="#" class="button" target="_self">MORE</a>*@
                                                            </div>

                                                        </li>
                                                    }
                                                </ul>
                                            </div><ol class="flex-control-nav flex-control-paging">
                                                @for (int i = 2; i < Model.FavDests.Count; i++)
                                                {
                                                    <li><a class="flex-active">@i</a></li>
                                                }
                                            </ol><ul class="flex-direction-nav"><li><a class="flex-prev" href="#">Previous</a></li><li><a class="flex-next" href="#">Next</a></li></ul>
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="description text-center">

                                            <h1 class="lead">No Available Favourite Destinations Yet</h1>

                                        </div>
                                    }
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

        </div>
        @if (Model.FavDests != null && Model.FavDests.Count > 0)
        {
            foreach (var favDest in Model.FavDests)
            {
                <div class="modal fade" id="ModalCustomers-@favDest.Id" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                <h4 class="modal-title">@favDest.DestinationName</h4>
                            </div>
                            <div class="modal-body">
                                <p>@favDest.Description</p>
                                <img title="Test Pack" class="img-fluid" src="@favDest.ImgName" />
                            </div>
                        </div>
                    </div>
                </div>
            }
        }

        </section>

@section IndexPage {
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {

            if (window.location.hash == "#NexusServices") {
                $('html,body').animate({
                    scrollTop: $("#NexusServices").offset().top
                },1000);
            }

            $("#CitiesNames").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: "/Home/AutoGetCities",
                        type: "POST",
                        dataType: "json",
                        data: { Prefix: request.term },
                        success: function (data) {
                            response($.map(data, function (value, key) {
                                return {
                                    label: value.CountryName + ", " + value.Name,
                                    value: value.Runno
                                };
                            }))

                        }
                    })
                },
                minLength: 2,
                focus: function (event, ui) {
                    event.preventDefault();
                    $('#CitiesNames').val(ui.item.label);
                    return false;
                },
                select: function (event, ui) {
                    event.preventDefault();
                    $("#CitiesNames").val(ui.item.label);
                    $("#RunoId").val(ui.item.value);
                },
                messages: {
                    noResults: "", results: ""
                }
            }).data('ui-autocomplete')._renderItem = function (ul, item) {
                ul.css({
                    'height': '150px',
                    'overflow': 'auto'
                });
                return $("<li class='ui-autocomplete-row' ></li>").data("item.autocomplete", item).append(item.label).appendTo(ul);
                };
            $("#CountriesNames").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: "/Home/AutoGetCountries",
                        type: "POST",
                        dataType: "json",
                        data: { Prefix: request.term },
                        success: function (data) {
                            response($.map(data, function (value, key) {
                                return {
                                    label: value.Key,
                                };
                            }))

                        }
                    })
                },
                minLength: 1,
                focus: function (event, ui) {
                    event.preventDefault();
                    $('#CountriesNames').val(ui.item.label);
                    return false;
                },
                select: function (event, ui) {
                    event.preventDefault();
                    $("#CountriesNames").val(ui.item.label);
                },
                messages: {
                    noResults: "", results: ""
                }
            }).data('ui-autocomplete')._renderItem = function (ul, item) {
                ul.css({
                    'height': '150px',
                    'overflow': 'auto'
                });
                    return $("<li class='ui-autocomplete-row' ></li>").data("item.autocomplete", item).append(item.label).appendTo(ul);
                };

            
            
        });
    </script>
}

<!-- End Of Index Page -->
