                                            </a>
                                        </div>
                                        @for (int i = 1; i < Model.Packages.Count; i++)
                                        {

                                            <div class="item">
                                                <p>@Model.Packages[i].Title</p>
                                                <a href="/adminmanage/PackageDetatils/@Model.Packages[i].Id">
                                                    <img title="@Model.Packages[i].Title" width="222" height="222" class="image-100" src="@Model.Packages[i].MainImageName" />
                                                </a>
                                            </div>

                                        }



                                    </div>
                                </div>

                            }
                            else
                            {
                                <p class="lead text-center">No New Packages Yet</p>
                            }
